{
    "name": "objectionjs-repository",
    "version": "1.0.11",
    "description": "ObjectionJS Repository is repository pattern implementation on top of KnexJS and ObjectionJS",
    "main": "build/index.js",
    "files": [
        "build/**/*"
    ],
    "scripts": {
        "build": "tsc",
        "format": "prettier --config .prettierrc 'src/**/*.ts' --write",
        "lint": "eslint . --ext .ts",
        "lint:fix": "eslint . --ext .ts --fix",
        "prepare": "npm run build",
        "prepublishOnly": "npm run lint",
        "preversion": "npm run lint && npm run test",
        "test": "mocha -r ts-node/register src/test/helpers/init.ts src/test/**/*.test.ts"
    },
    "keywords": [
        "KnexJS",
        "ObjectionJS",
        "Database",
        "Repository Pattern"
    ],
    "author": "Ahmed Adel",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/AhmedAdelFahim/objectionjs-repository/issues"
    },
    "homepage": "https://github.com/AhmedAdelFahim/objectionjs-repository#readme",
    "devDependencies": {
        "@types/mocha": "^10.0.6",
        "@types/node": "^18.11.18",
        "@types/ramda": "^0.28.22",
        "@typescript-eslint/eslint-plugin": "^5.49.0",
        "@typescript-eslint/parser": "^5.49.0",
        "dotenv": "^16.4.1",
        "eslint": "^8.33.0",
        "eslint-config-prettier": "^8.6.0",
        "eslint-plugin-prettier": "^4.2.1",
        "mocha": "^10.2.0",
        "pg": "^8.11.3",
        "prettier": "^2.8.3",
        "rimraf": "^4.1.2",
        "ts-node": "^10.9.2",
        "ts-node-dev": "^2.0.0",
        "typescript": "^4.9.4"
    },
    "dependencies": {
        "knex": "^2.5.1",
        "ramda": "^0.28.0",
        "objection": "^3.1.5"
    }
}
